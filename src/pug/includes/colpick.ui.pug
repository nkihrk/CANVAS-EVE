div#colpick
  input#input-colpick(
    class="ellipsis input-prefix"
    placeholder=""
    type="text"
    spellcheck="false")

  div#color-colpick

  div#rgb-colpick
    div#r-colpick
      div R
      div.bar-colpick
        div.colbar-colpick
        div#red-cir-colpick(class="bartop-colpick")

      input#input-r-colpick(class="num-colpick input-prefix")

    div#g-colpick
      div G
      div.bar-colpick
        div.colbar-colpick
        div#green-cir-colpick(class="bartop-colpick")

      input#input-g-colpick(class="num-colpick input-prefix")

    div#b-colpick
      div B
      div.bar-colpick
        div.colbar-colpick
        div#blue-cir-colpick(class="bartop-colpick")

      input#input-b-colpick(class="num-colpick input-prefix")




